{
  "rewrites": [
    {
      "source": "/certificado",
      "destination": "/index.html"
    },
    {
      "source": "/certificados/(.+)",
      "destination": "/index.html"
    },
    {
      "source": "/quem-somos",
      "destination": "/index.html"
    },
    {
      "source": "/membro/:id",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/certificados/(.*)\\.pdf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        }
      ]
    }
  ]
}